<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.homeController">
    <children>
        <Pane prefHeight="500.0" prefWidth="1000.0">
            <children>
                <TextField fx:id="toUserField" alignment="TOP_CENTER" layoutX="14.0" layoutY="405.0" promptText="To" />
                <TextField fx:id="messageField" alignment="TOP_CENTER" layoutX="180.0" layoutY="405.0" prefHeight="25.0" prefWidth="643.0" promptText="Message" />
                <Button layoutX="837.0" layoutY="405.0" mnemonicParsing="false" onAction="#sendButton" prefHeight="25.0" prefWidth="149.0" text="Send" />
                <Label fx:id="consoleText" alignment="CENTER" layoutX="426.0" layoutY="457.0" prefHeight="17.0" prefWidth="149.0" text="Enter the following" textAlignment="CENTER" />
                <Button fx:id="logoutButton" layoutX="809.0" layoutY="34.0" mnemonicParsing="false" onAction="#logoutMethod" prefHeight="25.0" prefWidth="149.0" text="Log Out" />
                <Label alignment="TOP_CENTER" layoutX="597.0" layoutY="457.0" prefHeight="17.0" prefWidth="149.0" text="or" textAlignment="CENTER" />
                <Label fx:id="nameLabel" alignment="TOP_CENTER" layoutX="17.0" layoutY="27.0" prefHeight="39.0" prefWidth="407.0" text="NICKNAME - FULL NAME" textAlignment="CENTER" textFill="#9e2828">
                    <font>
                        <Font name="Droid Sans Bold" size="32.0" />
                    </font>
                </Label>
            <Button layoutX="482.0" layoutY="34.0" mnemonicParsing="false" onAction="#showLogs" prefHeight="25.0" prefWidth="149.0" text="Log" />
            <Button layoutX="645.0" layoutY="34.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Chat" />
            <TextFlow layoutX="44.0" layoutY="117.0" prefHeight="200.0" prefWidth="200.0" />
            </children>
        </Pane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>


</GridPane>
